let h=1;  
function mcm (a,b){
let i=2;
while(i<=a || i<=b){
if(a%i==0 || b%i==0){
  h*=i;
 	if(a%i==0)
    a=a/i;
  if(b%i==0)  
    b=b/i;
  }
  else
  i++;
}
return h;  
}
let a=prompt("Ingrese un numero "),b=prompt("Ingrese otro numero ");
let c= mcm(a,b);
document.write( " minimo comun  multiplo  " + c );
