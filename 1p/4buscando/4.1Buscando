<script>
function linearSearch(arr, key){
    for(let i = 0; i < arr.length; i++){
        if(arr[i] === key){
            return i
        }
    }
    return -1
}
let arr=["hola","hello","hi","good evening","happy"];
let key=prompt("Busca una palabra de estas: "+arr);  
let hey=linearSearch(arr,key);  
if(hey==false)  
document.write("La palabra "+key+" Esta en el Ã­ndice "+hey);  
else  
document.write(hey);  
</script>
